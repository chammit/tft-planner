{
	"env": {
		"es6": true,
		"node": true
	},
	"extends": [
		"eslint:recommended"
	],
	"parserOptions": {
		"sourceType": "module",
		"ecmaVersion": 8
	},
	"globals": {
		"io": true
	},
	"rules": {
		"indent": [
			0
		],
		"no-redeclare": [
			0
		],
		"quotes": [
			"error",
			"single"
		],
		"semi": [
			"error",
			"always"
		],
		"no-console": [
			0
		],
		"no-unused-vars": "warn"
	}
}